                <thead>
                  <tr style="background-color: #3c8dbc; color: white;">
                    <th>#</th>
                    <th>Название</th>
                    <th class="NOTMOBILE">Цена</th>
                    <th class="NOTMOBILE">Активность</th>
                    <th class="NOTMOBILE">Действия</th>
                  </tr>
                </thead>
                <tbody>
                  <% for(var i = 0; i < tovar.length; i++){ %>
                    <tr class="tovar-ai-<%= tovar[i].AI %>">
                      <td>
                        <%= tovar[i].AI %>
                      </td>
                      <td>
                        <%= tovar[i].title %>
                      <small>
                        <div class="miniTitleNameIcon"></div>

                      </small>
                      </td>
                      <td class="NOTMOBILE">
                        <%= tovar[i].price %> UAH
                          <div class="sale">
                            <% if(tovar[i].sale[0]){ %>
                              Скидка:
                              <%= tovar[i].sale[1] %> %
                                <% } %>
                          </div>
                      </td>

                      <% if(tovar[i].availability){ %>
                        <td class="tovar_active NOTMOBILE" style="background-color: #169814; text-align: center; color: white">
                          Товар активен
                          <div class="visibility_off" title="Выключить товар" onclick="ADMIN.TOVAR_VISIBILITY(false, <%= tovar[i].AI %>); $(this).parent().css({'background-color': '#981414'}).removeClass('tovar_active').addClass('tovar_none_active')"></div>
                        </td>
                        <% } else { %>
                          <td class="tovar_none_active NOTMOBILE" style="background-color: #981414; text-align: center; color: white">
                            Товар неактивен
                            <div class="visibility_on" title="Включить товар" onclick="ADMIN.TOVAR_VISIBILITY(true,<%= tovar[i].AI %>);  $(this).parent().css({'background-color': '#169814'}).removeClass('tovar_none_active').addClass('tovar_active')"></div>
                          </td>
                          <% } %>

                          <td class="">
                            <a class="btn btn-app" title="Редактировать" onclick="ADMIN.EDIT_TOVAR(<%= tovar[i].AI %>)">
                              <i class="fa fa-edit"></i>
                            </a>
                            <a class="btn btn-app" title="Удалить" onclick="ADMIN.TOVAR_REMOVE(<%= tovar[i].AI %>)">
                              <i class="fa fa-ban" ></i>
                            </a>
                          </td>
                        </tr>
                    <% } %>
                </tbody>
                <tfoot>

                </tfoot>
              </table>
