      <!-- настройки сайта -->
      <div class="pageOfPanel">
        <section class="content-header">
          <h1>настройки сайта</h1>
        </section>
        <section class="content">
          <div class="col-md-6">
            <div class="box box-primary">
              <div class="box-header with-border">
                <h3 class="box-title">Работа сайта</h3>
              </div>
              <form role="form">
                <div class="box-body">
                  <div class="form-group">

                    <a class="btn btn-app" onclick="ADMIN.SITE_STATUS(0)">
                    <i class="fa fa-play"></i> Старт</a>

                    <a class="btn btn-app" onclick="ADMIN.SITE_STATUS(1)">
                    <i class="fa fa-pause"></i>Остановить</a>

                    <a class="btn btn-app" onclick="ADMIN.SITE_STATUS(2)">
                    <i class="fa fa-repeat"></i>Обновить</a>

                    <a class="btn btn-app" onclick="ADMIN.SITE_STATUS(3)">
                    <i class="fa fa-bolt"></i>перезапустить</a>

                  </div>
                </div>
              </form>
            </div>

            <div class="box box-primary">
              <div class="box-header with-border">
                <h3 class="box-title">Заголовок сайта</h3>
              </div>
              <form role="form">
                <div class="box-body">
                  <div class="form-group">
                    <label for="exampleInputEmail1">Заголовок по Русски</label>
                    <input type="text" class="ru_lang form-control RU_PIC" id="edit_title_ru" value="<%= titles[0].index %>" placeholder="Введите заголовок сайта">
                  </div>
                  <div class="form-group">
                    <label for="exampleInputPassword1">Заголовок по украински</label>
                    <input type="text" class="ua_lang form-control UA_PIC" id="edit_title_ua" value="<%= titles[1].index %>" placeholder="Введіть заголовок сайту">
                  </div>
                </div>
                <div class="box-footer">
                  <button type="submit" class="btn btn-block btn-success btn-sm save_pic" onclick="ADMIN.SAVE_TITLE()">Сохранить</button>
                </div>
              </form>
            </div>
          </div>

          <div class="col-md-6">
            <div class="box box-primary">
              <div class="box-header with-border">
                <h3 class="box-title">Настройка логотипа</h3>
              </div>
