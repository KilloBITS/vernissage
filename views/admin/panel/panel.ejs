                        <%= users[i].email %>
                        <div class="sendUserMessage" mail="<%= users[i].email %>" onclick="ADMIN.SEND_USER_MESSAGE('<%= users[i].email %>')"></div>
                      </td>
                      <td class="NOTMOBILE">
                        <%= users[i].phone_number %>
                        <div class="type_operator_<%= i %>"></div>
                        <script>
                        var test_str = "<%= users[i].phone_number %>";
                        var start_pos = test_str.indexOf('(') + 1;
                        var end_pos = test_str.indexOf(')',start_pos);
                        var text_to_get = test_str.substring(start_pos,end_pos);

                        switch(text_to_get){
                          case "066": $(".type_operator_<%= i %>").addClass("mts").attr("title","VODAFONE"); break;
                          case "099": $(".type_operator_<%= i %>").addClass("mts").attr("title","VODAFONE"); break;
                          case "050": $(".type_operator_<%= i %>").addClass("mts").attr("title","VODAFONE"); break;
                          case "063": $(".type_operator_<%= i %>").addClass("Life").attr("title","LIFECELL"); break;
                          case "073": $(".type_operator_<%= i %>").addClass("Life").attr("title","LIFECELL"); break;
                          case "097": $(".type_operator_<%= i %>").addClass("Kievstar").attr("title","Киевстар"); break;
                          case "098": $(".type_operator_<%= i %>").addClass("Kievstar").attr("title","Киевстар"); break;
                          case "067": $(".type_operator_<%= i %>").addClass("Kievstar").attr("title","Киевстар"); break;
                          default: console.log("none"); break;
                        }
                        </script>
                      </td>
                      <td class="NOTMOBILE">
                        <%= users[i].secret %>
                      </td>
                      <td class="NOTMOBILE">
                        <%= users[i].rank %>
                      </td>
                      <td class="NOTMOBILE">
                        <%= users[i].stars %>
                      </td>
                      <% if( users[i].isAdmin || users[i].isAdmin === 'true'){ %>
                      <td class="NOTMOBILE">
                        Администратор
                      </td>
                      <% }else{ %>
                        <td class="NOTMOBILE">
                          Пользователь
                        </td>
                      <% } %>
                      <td class="NOTMOBILE">
                        <a class="btn btn-app" title="Заблокировать" onclick="ADMIN.BANNED(<%= users[i].AI %>)">
                          <i class="fa fa-remove"></i>
                        </a>
                        <a class="btn btn-app" title="Редактировать" onclick="ADMIN.EDIT_USER(<%= users[i].AI %>)">
                          <i class="fa fa-edit"></i>
                        </a>
                      </td>
                  <% } %>
                </tbody>
                <tfoot>

                </tfoot>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
        </section>
