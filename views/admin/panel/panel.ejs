                    <img id="vis_logo" src="../../../image/<%= conf.site_logo %>">
                    <input type="file" id="newLogotype" style="display: none;">
                  </div>
                </div>
                <div class="box-footer">
                  <button type="submit" class="btn btn-block btn-success btn-sm image_pic" onclick="$('#newLogotype').click()">Выберите изображение</button>
                </div>
              </form>
            </div>

            <div class="box box-primary">
              <div class="box-header with-border">
                <h3 class="box-title">Соц. сети</h3>
              </div>
              <form role="form" onsubmit="return false">
                <div class="box-body">
                  <div class="form-group">
                    <label for="exampleInputEmail1">ВКонтакте</label>
                    <input type="text" class="ru_lang form-control" id="VK" value="<%= conf.socials[0].link %>" placeholder="Введите URL на страницу">
                  </div>

                  <div class="form-group">
                    <label for="exampleInputPassword1">INSTAGRAM</label>
                    <input type="text" class="ua_lang form-control" id="INSTA" value="<%= conf.socials[1].link %>" placeholder="Введите URL на страницу">
                  </div>

                  <div class="form-group">
                    <label for="exampleInputPassword1">FACEBOOK</label>
                    <input type="text" class="ua_lang form-control" id="FB" value="<%= conf.socials[2].link %>" placeholder="Введите URL на страницу">
                  </div>
                </div>

                <div class="box-footer">
                  <button type="submit" class="btn btn-block btn-success btn-sm save_pic" onclick="ADMIN.SAVE_SOCIALS()">Сохранить</button>
                </div>
              </form>
            </div>

          </div>
        </section>
